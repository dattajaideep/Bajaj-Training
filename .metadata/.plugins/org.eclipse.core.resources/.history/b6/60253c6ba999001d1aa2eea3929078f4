package finserv.day3.taskexceptions;

import java.util.ArrayList;
import java.util.LinkedHashSet;
import java.util.Scanner;
import java.util.Set;

public class Shop {
private static ArrayList<Integer> Items=new ArrayList<Integer>();
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
	}
}

class World{
	String Id;//Only one Id string object gets created
	String Name;
	String price;
	public World(String Id,String Name,String price){
		this.Id=Id;
		this.Name=Name;
		this.price=price;
	}
	public static void setCreate(boolean b) {
		Set<String> set=new LinkedHashSet<String>();
		set.add(Id);
		set.add(Name);
		set.add(price);
		
	}
}
