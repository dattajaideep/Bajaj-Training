package com.unext.inheritance;

public class Parent {
    protected int p = 22;
    public Parent(){
        System.out.println("In parent non paramaterised constructor");
    }
    public Parent(int p){
        System.out.println("In parent paramertrised ");
        this.p = p;
    }
    public void show() {
        p = 10;
        System.out.println("in parent class");
    }
}